<!DOCTYPE html>
<html lang="pt-BR">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/pagina-inicial-logged.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css" />
    <link rel="shortcut icon" href="imagens/logo-laranja.png" type="image/x-icon">
    <title>Dimittens</title>
</head>

<body>
    <% if (pagina === 'homelogged' && autenticado) { %>
        <% if (autenticado.tipo.toLowerCase() === 'psicologo') { %>
            <%- include("../partial/dashboardpsicologo") %>
        <% } else { %>
            <%- include("../partial/homelogged") %>
        <% } %>
    <% } else { %>
        <%- include("../partial/" + pagina, { valores: typeof valores !== 'undefined' ? valores : {} }) %>
    <% } %>
</body>

</html>
